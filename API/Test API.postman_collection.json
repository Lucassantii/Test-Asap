					"raw": "http://api.openweathermap.org/geo/1.0/direct?q=São Paulo&appid=cd1622bc61f17c289c2218e3a8c3718b",
					"protocol": "http",
					"host": [
						"api",
						"openweathermap",
						"org"
					],
					"path": [
						"geo",
						"1.0",
						"direct"
					],
					"query": [
						{
							"key": "q",
							"value": "São Paulo"
						},
						{
							"key": "appid",
							"value": "cd1622bc61f17c289c2218e3a8c3718b"
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Validação latitude e longitude",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Validar status, lon, lat\", function () {\r",
							"\r",
							"   pm.expect(pm.response.json().coord.lon).to.be.eql(-46.6334); \r",
							"\r",
							"   pm.expect(pm.response.json().coord.lat).to.be.eql(-23.5507); \r",
							"\r",
							"});\r",
							"pm.test(\"Validar status, id\", function () {\r",
							"\r",
							"   pm.expect(pm.response.json().weather[0].id).to.be.eql(501); \r",
							"\r",
							"   \r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
